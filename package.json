    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "playwright": "^1.58.0",
    "prettier": "^3.1.1",
    "storybook": "^10.2.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^4.0.18"
  }
